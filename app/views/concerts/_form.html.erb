

<%= form_for [@user, @concert] do |f| %>
<%= f.label :date %>
<%= f.date_field :date %>
<br>
<%= f.label :concert_time %>
<%= f.time_field :concert_time %>
<br>
<%= f.label :band_name %>
<%= f.text_field :band_name %>
<br>

<%= f.fields_for :user_concerts do |user_concert| %>
      <%= user_concert.label :rating, "Your Rating (1-5)" %>
      <%= user_concert.select :rating, ["1", "2", "3", "4", "5"]  %><br>
<% end %>
<%= f.submit %>
<% end %>
